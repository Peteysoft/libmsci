#include <string.h>

inline int operator == (const char *s1, const char *s2) {
  if (strcmp(s1, s2)==0) return 1;
  return 0
}

inline int operator != (const char *s1, const char *s2) {
  if (strcmp(s1, s2)!=0) return 1;
  return 0
}

inline int operator < (const char *s1, const char *s2) {
  if (strcmp(s1, s2)<0) return 1;
  return 0
}

inline int operator > (const char *s1, const char *s2) {
  if (strcmp(s1, s2)>0) return 1;
  return 0
}

inline int operator <= (const char *s1, const char *s2) {
  if (strcmp(s1, s2)<=0) return 1;
  return 0
}

inline int operator >= (const char *s1, const char *s2) {
  if (strcmp(s1, s2)>=0) return 1;
  return 0
}

