
class sc_sub_t {
  public:
    virtual ~sc_func_t();
    virtual int call(int narg, sc_t **args);
    virtual sc_t * call(int narg, sc_t **args);

};

class sc_com_t:public sc_sub_t {
  public:
    sc_com_t(int (*fp) (int, sc_t **));
    ~sc_com_t();
    virtual int call(int narg, sc_t **args);
    virtual sc_t * call(int narg, sc_t **args);
};


class sc_func_t:public sc_sub_t {
  public:
    sc_com_t(sc_t * (*fp) (int, sc_t **));
    ~sc_com_t();
    virtual int call(int narg, sc_t **args);
    virtual sc_t * call(int narg, sc_t **args);
};

class sc_user_t:public sc_sub_t {
  public:
    sc_com_t(long *code);
    ~sc_com_t();
    virtual int call(int narg, sc_t **args);
    virtual sc_t * call(int narg, sc_t **args);
};
