all: agf2/agf_paper.pdf multiborders/multiborders_arxiv.pdf directions/agf_directions.pdf

clean:
	make -C agf2 clean
	make -C multiborders clean
	make -C directions clean

agf2/agf_paper.pdf:
	make -C agf2

multiborders/multiborders_arxiv.pdf:
	make -C multiborders

directions/agf_directions.pdf:
	make -C directions
